					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('jinblobcontainerlink_accountKey')]"
					}
				},
				"connectVia": {
					"referenceName": "jinintegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/jinintegrationRuntime')]"
			]
		},
